{
    "name": "mkhab7/v2board-sdk",
    "description": "PHP SDK for V2Board API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mkhab7",
            "email": "info@mkhab7.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.0",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.36",
        "pestphp/pest-plugin-mock": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Mkhab7\\V2Board\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mkhab7\\V2Board\\SDK\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
